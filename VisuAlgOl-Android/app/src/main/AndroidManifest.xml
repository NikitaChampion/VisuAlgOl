<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.olympiad_algorithms.visualgol_android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Algorithms"
            android:screenOrientation="portrait" />
        <activity
            android:name=".StupidSort"
            android:screenOrientation="portrait" />
        <activity
            android:name=".BubbleSort"
            android:screenOrientation="portrait" />
        <activity
            android:name=".QuickSort"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MergeSort"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Prefix_fun"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Z_fun"
            android:screenOrientation="portrait" />
        <activity android:name=".SelectionSort" />
        <activity android:name=".Graph" />
        <activity android:name=".LinSearch"></activity>
    </application>

</manifest>