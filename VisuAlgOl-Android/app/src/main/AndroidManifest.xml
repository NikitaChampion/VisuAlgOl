<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.olympiad_algorithms.visualgol_android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Algorithms" />
        <activity android:name=".StupidSort" />
        <activity android:name=".BubbleSort" />
        <activity android:name=".HeapSort" />
        <activity android:name=".QuickSort" />
        <activity android:name=".MergeSort" />
        <activity android:name=".ShellSort"></activity>
    </application>

</manifest>